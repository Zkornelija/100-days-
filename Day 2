#Task 2 
height = input("enter your height in m: ")
weight = input("enter your weight in kg: ")
 
a = float(height)
b = float(weight)

BMI = b / a**2 
print(int(BMI))

#Task 3 
age = input("What is your current age?")

age_number = 90 - int(age)
months = age_number * 12 
weeks = age_number * 52 
days = age_number * 365

print(f"You have {days} days, {weeks} weeks, and {months} months left.")

# Task 4 
print("Welcome to the tip calculator")
total = float(input("What was the total bill? "))
procentages = int(input("What precentage tip would you like to give? 10, 12, or 15? "))
people = int(input("How many people to split bill? "))
procentages_all = (100 + procentages) / 100 
total_for_person = round(total * procentages_all / people, 2) 
print(f"Each person should pay: ${total_for_person}")

